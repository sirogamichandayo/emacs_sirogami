#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main(int argc, char **argv)
{
	int N; cin >> N;
	vector<int> h(N); for (int i = 0; i < N; ++i) cin >> h[i];

	int i{0};
	while (true)
	{
		if (*max_element(h.begin(), h.end()) == 0) break;
		
		while (i < N)
		{
			if (h[i] == 0)
			{
				++i;
				continue;
			}

		
		}
	}

	
}

#include <bits/stdc++.h>
using namespace std;
using ll = long long;
constexpr ll MOD = 1000000007;


int main(int argc, char **argv)
{
	ll res{0};
	ll N, K; cin >> N >> K;
	vector<bool> has(N+1, false);
	for (ll i = 0; i < K; ++i)
	{
		ll d; cin >> d;
		for (ll j = 0;j < d; ++j)
		{
			ll a; cin >> a;
			has[a] |= true;
		}
	}
	
}

#include <bits/stdc++.h>
using namespace std;
using ll = long long;
constexpr ll MOD = 1000000007;
constexpr ll INF = 1ll<<60;

char field[100][100];
ll d[100][100];

int main(int argc, char **argv)
{
	ll H, W; cin >> H >> W;
	ll cb{0}, cw{0};
	for (ll h = 0; h < H; ++h) for (ll w = 0; w < W; ++w) {
			cin >> field[h][w];
			if (field[h][w] == '.') ++cw;
			else ++cb;
		}

	

	
}

#include <bits/stdc++.h>
using namespace std;


int main(int argc, char **argv)
{
	string str; cin >> str;
	int N = str.size();
	string sample1(N, '0');
	string sample2(N, '0')
	for (int i = 0; i < N; ++i)
	{
		if (i & 0)
			sample1[i] = '1';
		if (i & 1)
			sample2[i] = '1';

	}

	std::cout << sample1<< std::endl;
	std::cout << sample2<< std::endl;
}

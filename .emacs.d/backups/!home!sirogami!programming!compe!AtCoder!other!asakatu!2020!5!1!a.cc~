#include <bits/stdc++.h>
using namespace std;
using ll = long long;
constexpr ll MOD = 1000000007;
int main(int argc, char **argv)
{
	ll a, b, c; cin >> a >> b >> c;
	book ok{false};
	if (a + b == c) ok = true;
	if (a + c == b) ok = true;
	if (a = b + c) ok = true;
	std::cout << (ok ? "Yes" : "No")<< std::endl;
}

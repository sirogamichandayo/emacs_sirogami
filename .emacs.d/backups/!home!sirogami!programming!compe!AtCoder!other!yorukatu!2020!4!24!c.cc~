#include<bits/stdc++.h>
using namespace std;
using ll = long long;
constexpr ll MOD = 1000000007;
constexpr ll INF = 1ll<<60;

int main(int argc, char **argv)
{
	ll N; cin >> N;
	vector<ll> A(N);
	for (ll i = 0; i < N; ++i) cin >> A[i];
	vector<ll> sum(N+1, 0);
	for (ll i = 0; i < N; ++i)
		++sum[A[i]];

	
	
}

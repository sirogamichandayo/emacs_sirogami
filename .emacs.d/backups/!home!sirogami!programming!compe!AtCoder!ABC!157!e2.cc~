#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using P = pair<ll, ll>;

const ll MOD = 1e9+7;
const ll INF = 1ll<<60;

#define FOR(i,a,b) for (ll i=(a);i<(ll)(b);++i)
#define REP(i,n) FOR(i,0,n)
#define ALL(v) (v).begin(),(v).end()

ll N, Q;
string S;
vector<set<ll>> V(26);
int main(int argc, char **argv)
{
	cin >> N >> S >> Q;
	REP(i, N)
		V[S[i] - 'a'].insert(i);

	
}

#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main(int argc, char **argv)
{
	ll n, m;
	cin >> n >> m;
	vector<pair<ll, ll>> a_b(n);
	for (int i = 0; i < n; ++i)
	{
		ll a, b; cin >> a >> b;
		a_b[i] = pair<ll, ll>(a, b);
	}

	sort(a_b.begin(), a_b.end(), [](const auto &a, const auto &b)
								 {
									 return a.first < b.first; 
								 });

	
}

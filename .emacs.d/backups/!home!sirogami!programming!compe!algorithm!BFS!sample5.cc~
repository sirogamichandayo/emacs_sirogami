#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using Graph = vector<vector<int>>;

int main(int argc, char **argv)
{
	ll N; cin >> N;
	Graph G(N);
	vector<int> deg(N, 0);
	for (int i = 0; i < N; ++i)
	{
		ll a, b; cin >> a >> b; --a, --b;
		G[a].push_back(b);
		G[b].push_back(a);
		++deg[a]; ++deg[b];
	}

	
		
		
}

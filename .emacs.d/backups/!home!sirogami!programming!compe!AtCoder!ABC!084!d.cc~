#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int q;
vector<int> l, r;

ll GCD(ll a, ll b)
{
	if (b == 0) return a;
	else return GCD(b, a%b);
}

int main(int argc, char **argv)
{
	cin >> q;
	l.resize(q+10);
	v.resize(q+10);

	for (int i = 0; i < q; ++i)
		cin >> l[i] >> r[i];

	
}
